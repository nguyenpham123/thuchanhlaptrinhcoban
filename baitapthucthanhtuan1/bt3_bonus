#include <iostream>
using namespace std;

int main() {
    int a, b;
    
    cout<< "Nhap vao so nguyen duong a: ";
    cin>>a;
    
    cout<< "Nhap vào so nguyen duong b: ";
    cin>>b;
    
    int quotient = a / b;
    int remainder = a % b;
    
    cout << "Ket qua chia lay phan nguyen: " << quotient << endl;
    cout << "Phan du: " << remainder << endl;
    
    return 0;
}