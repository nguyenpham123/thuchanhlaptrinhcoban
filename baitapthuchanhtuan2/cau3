#include <iostream>
//bt3
//0850080086 PHAM TU KHOI NGUYEN 08_DH_THMT
using namespace std;
int main() {
    const int hienthiquatrangbangma = 20;
    cout << "BANG MA ASCII" << endl;
    for (int i = 0; i < 128; ++i) {
        cout << static_cast<char>(i) << "\t" << i << "\t";
        if ((i + 1) % hienthiquatrangbangma == 0) {
            cout << "\nNhan Enter de xem trang tiep theo...";
            cin.ignore();
            cin.get();
        }
    }
    cout << endl;
    return 0;
}