#include <iostream>
#define max(a,b) ((a)>(b)?(a):(b))
#define min(a,b) ((a)<(b)?(a):(b))
using namespace std;
int main(){
    int a,b;
    cout<<" Nhap a: ";
    cin>>a;
    cout<<" Nhap b: ";
    cin>>b;
    if(a==b){
        cout<<"a=b= "<<a;
    }
    else{
        cout<<"\nMax = "<<max(a,b);
        cout<<"\nMin = "<<min(a,b);
    }
    return 0;
}